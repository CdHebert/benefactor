<body>





  <div class="limiter">
    <div class="container-login100" style="background-image: url('/images/lights.jpg');">
      <div class="wrap-login100 p-t-30 p-b-50">
        <span class="login100-form-title p-b-41">
          Account Login
        </span>
        <form class="login-form login100-form validate-form p-b-33 p-t-5">

          <div class="wrap-input100 validate-input" data-validate="Enter username">
            <input id="username-login" class="input100" type="text" name="username" placeholder="User name">
            <span class="focus-input100" data-placeholder="&#xe82a;"></span>
            {{!-- <label for="username-login">username:</label>
            <input type="text" id="username-login" /> --}}
          </div>

          <div class="wrap-input100 validate-input" data-validate="Enter password">
            <input id="password-login" class="input100" type="password" name="pass" placeholder="Password">
            <span class="focus-input100" data-placeholder="&#xe80f;"></span>
            {{!-- <label for="password-login">password:</label>
            <input type="password" id="password-login" /> --}}
          </div>

          <div class="container-login100-form-btn m-t-32">
            <button type="submit" class="hvr-glow btn login100-form-btn">
              Login
            </button>
          </div>

        </form>
        <span class="login100-form-title p-b-41">
          Account Signup
        </span>
        <form class="signup-form login100-form validate-form p-b-33 p-t-5">

          <div class="wrap-input100 validate-input" data-validate="Enter username">
            <input id="username-signup" class="input100" type="text" name="username" placeholder="User name">
            <span class="focus-input100" data-placeholder="&#xe82a;"></span>
            {{!-- <label for="username-signup">username:</label>
            <input type="text" id="username-signup" /> --}}
          </div>

          <div class="wrap-input100 validate-input" data-validate="Enter password">
            <input id="password-signup" class="input100" type="password" name="pass" placeholder="Password">
            <span class="focus-input100" data-placeholder="&#xe80f;"></span>
            {{!-- <label for="password-signup">password:</label>
            <input type="password" id="password-signup" /> --}}
          </div>

          <div class="container-login100-form-btn m-t-32">
            <button type="submit" class="hvr-glow btn login100-form-btn">
              Signup
            </button>
          </div>

        </form>
      </div>
    </div>
  </div>


